<h1>Admin</h1>
<%= link_to "Go back", movies_path %> | <%= link_to "Create User", new_admin_user_path %>
<hr>

<%= paginate @users %>
<% @users.each do |user| %>
  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Admin</th>
        <th>Created At</th>
        <th>Updated At</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td><%= user.firstname + ' ' + user.lastname%></td>
        <td><%= user.email %></td>
        <td><%= user.admin %></td>
        <td><%= user.created_at %></td>
        <td><%= user.updated_at %></td>
        <td><%= link_to "Edit User", edit_admin_user_path(user) %></td>
      </tr>
    </tbody>
  </table>
  <hr/>
<% end %>